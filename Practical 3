Code:
clc;clear;closeall;
img = imread('pears.png');if size(img, 3) == 3
grayImg=rgb2gray(img);else
grayImg=img;end
[hogFeatures,hogVisualization]=extractHOGFeatures(grayImg,'CellSize',[88],'NumBins', 9);
figure;subplot(1,2,1);imshow(img);
title('OriginalImage');subplot(1, 2, 2);imshow(img);
hold on;plot(hogVisualization);
title('HOGFeaturesVisualization');
fprintf('SizeofHOGfeaturevector:%d\n',numel(hogFeatures));
